import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a =sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
        int secondLargest;
        if ((a > b && a < c) || (a > c && a < b)) {
            secondLargest = a;
        } else if ((b > a && b < c) || (b > c && b < a)) {
            secondLargest = b;
        } else {
            secondLargest = c;
        }

        int gcd = findGCD(a, findGCD(b, c));
        System.out.println("The treasure is in the box which has number " + secondLargest + ".");
        System.out.println("The code to open the box is " + gcd + ".");
    }
    public static int findGCD(int x, int y) {
        while (y != 0) {
            int temp = y;
            y = x % y;
            x = temp;
        }
        return x;
    }
}
        
